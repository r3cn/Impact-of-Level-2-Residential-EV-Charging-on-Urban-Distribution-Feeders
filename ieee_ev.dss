! CAPACITORS
New Capacitor.Cap1 Bus1=675 phases=3 kVAR=600 kV=4.16 
New Capacitor.Cap2 Bus1=611.3 phases=1 kVAR=100 kV=2.4 

! SWITCH
New Line.671692 Phases=3 Bus1=671 Bus2=692 Switch=y r1=1e-4 r0=1e-4 x1=0.000 x0=0.000 c1=0.000 c0=0.000

//----------------------------------------------------------------------------------------------------------

! ****** set voltage bases, solve ******
Set Voltagebases=[115, 4.16, 0.48]
calcv

//----------------------------------------------------------------------------------------------------------

! ****** MONITORING SETUP ******

! Voltage monitors at critical buses
New Monitor.Voltage_633 Element=Transformer.XFM1 Terminal=1 Mode=0 
New Monitor.Voltage_634 Element=Transformer.XFM1 Terminal=2 Mode=0
New Monitor.Voltage_611 Element=Line.684611 Terminal=2 Mode=0
New Monitor.Voltage_652 Element=Line.684652 Terminal=2 Mode=0

New Monitor.XFM1_Power Element=Transformer.XFM1 Terminal=1 ppolar=no Mode=1    ! Power (kW, kvar)
! New Monitor.XFM1_Current Element=Transformer.XFM1 Terminal=1 Mode=4  ! Current magnitude
! New Monitor.XFM1_Loading Element=Transformer.XFM1 Terminal=1 Mode=0  ! Voltage for reference

! regulator tap monitors
New Monitor.RegControl1 Element=RegControl.Reg1 Terminal=1 Mode=0

//----------------------------------------------------------------------------------------------------------

! ****** RUN, PLOT ******

! Run daily simulation first
Set Mode=Daily
Set Number=24
Set Stepsize=1h
Solve

/*
Plot monitor object= voltage_633 channels=(1 3 5) bases=[2400 2400 2400]  !> pu conversion
Plot monitor object= voltage_634 channels=(1 3 5) bases=[277 277 277]       !> pu conversion
Plot monitor object= voltage_611 channels=(1) bases=[2401.77]
Plot monitor object= voltage_652 channels=(1) bases=[2401.77]
Plot monitor object= xfm1_power channels=(1 2 3 4 5 6)
*/

export monitor voltage_633
export monitor voltage_634
export monitor voltage_611
export monitor voltage_652
export monitor xfm1_power
export Monitor RegControl1

